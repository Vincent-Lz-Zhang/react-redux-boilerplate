{
  "name": "react-redux-boilerplate",
  "version": "3.0.0-beta.0",
  "description": "React Redux Boilerplate",
  "homepage": "https://github.com/xuqingkuang/react-redux-boilerplate",
  "author": {
    "name": "XQ Kuang",
    "email": "x@kxq.io"
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "jest",
    "scss",
    "sass",
    "boilerplate",
    "webpack-dev-server",
    "yarn",
    "hmr"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuqingkuang/react-redux-boilerplate"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --display-error-details --config webpack-production.config.js && uglifyjs ./assets/vendor.js > dist/vendor.js && rm ./dist/*.map",
    "buildvendor": "webpack --config webpack-vendor.config.js",
    "clean": "rimraf dist",
    "lint": "tslint --force \"src/**/*.ts*\" && sass-lint -v -q src/**/*.scss",
    "test": "jest --no-cache",
    "ghpages": "minify assets/index.html > dist/index.html && gh-pages -d dist"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "engines": {
    "npm": ">=5.0.3",
    "node": ">=6.11.0",
    "yarn": ">=0.24.5"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/node": "6.0.78",
    "@types/react": "^15.0.33",
    "@types/react-addons-test-utils": "^0.14.19",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.2",
    "@types/react-redux": "^4.4.45",
    "@types/react-router": "^4.0.12",
    "@types/react-router-dom": "^4.0.5",
    "codecov": "^2.2.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^20.0.4",
    "minify": "^2.1.8",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.9.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.2.0",
    "tslib": "^1.7.1",
    "tslint": "^5.4.3",
    "typescript": "2.3.2",
    "uglify-js": "^3.0.20",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-polyfill-service-plugin": "0.0.4"
  }
}
